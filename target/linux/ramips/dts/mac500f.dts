/dts-v1/;

#include "mt7621.dtsi"

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

/ {
	compatible = "tenbay,mac500f", "mediatek,mt7621-soc";
	model = "Tenbay MAC500F";

	aliases {
		led-boot = &led_blue;
		led-failsafe = &led_blue;
		led-running = &led_blue;
		led-upgrade = &led_blue;
	};

	memory@0 {
		device_type = "memory";
		reg = <0x0 0x8000000>;
	};

	chosen {
		bootargs = "console=ttyS0,57600";
	};

	gpio-leds {
		compatible = "gpio-leds";

		stat_r {
			label = "k2p:red:status";
			gpios = <&gpio0 13 GPIO_ACTIVE_HIGH>;
		};

		stat_y {
			label = "k2p:yellow:status";
			gpios = <&gpio0 14 GPIO_ACTIVE_LOW>;
		};

		led_blue: stat_b {
			label = "k2p:blue:status";
			gpios = <&gpio0 15 GPIO_ACTIVE_LOW>;
		};
	};

	// gpio-keys-polled {
	// 	compatible = "gpio-keys-polled";
	// 	poll-interval = <20>;

	// 	reset {
	// 		label = "reset";
	// 		gpios = <&gpio0 3 GPIO_ACTIVE_LOW>;
	// 		linux,code = <KEY_RESTART>;
	// 	};
	// };

	i2c0: i2c-gpio {
		compatible = "i2c-gpio";
		gpios = <&gpio0 3 GPIO_ACTIVE_HIGH /* sda */
		         &gpio0 4 GPIO_ACTIVE_HIGH /* scl */
		        >;
		#address-cells = <1>;
		#size-cells = <0>;

		expander0: pca9555@25 {
			compatible = "pca9555";
			reg = <0x25>;
		};
	};

	reg_core: reg-core {
		compatible = "regulator-fixed";
		regulator-name = "core";

		regulator-min-microvolt = < 500000>;
		regulator-max-microvolt = <3650000>;
		regulator-boot-on;
	};

	reg_io: reg-io {
		compatible = "regulator-fixed";
		regulator-name = "io";

		regulator-min-microvolt = <3000000>;
		regulator-max-microvolt = <3300000>;
		regulator-boot-on;
	};

	sfp5: sfp {
		compatible = "sff,sfp";
		i2c-bus = <&i2c0>;
		// mod-def0-gpio = <&expander0 5 GPIO_ACTIVE_LOW>;
		maximum-power-milliwatt = <1000>;
    };
};

&spi0 {
	status = "okay";

	m25p80@0 {
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <10000000>;
		m25p,chunked-io = <32>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "u-boot";
				reg = <0x0 0x30000>;
				read-only;
			};

			partition@30000 {
				label = "u-boot-env";
				reg = <0x30000 0x10000>;
				read-only;
			};

			factory: partition@40000 {
				label = "factory";
				reg = <0x40000 0x10000>;
				read-only;
			};

			partition@a0000 {
				compatible = "denx,uimage";
				label = "firmware";
				reg = <0x50000 0xfb0000>;
			};
		};
	};
};

&gdma {
	status = "okay";
};

&hsdma {
	status = "okay";
};

&eth {
	status = "okay";

	mtd-mac-address = <&factory 0xe006>;

	//switch mt7530
	gmac0: mac@0 {
		compatible = "mediatek,eth-mac";
		mtd-mac-address = <&factory 0x22>;
		reg = <0>;
		phy-mode = "trgmii";

		fixed-link {
			speed = <1000>;
			full-duplex;
			pause;
		};
	};

	mdio: mdio-bus {
		#address-cells = <1>;
		#size-cells = <0>;

		// reset-gpios = <&gpio0 7 GPIO_ACTIVE_LOW>; //reset timing dose not fit well

		mt7530: switch@0 {
			compatible = "mediatek,mt7530";
		    #address-cells = <1>;
			#size-cells = <0>;
			reg = <0>;
	    	pinctrl-names = "default";
		    mediatek,mcm;
			resets = <&rstctrl 2>;
		    reset-names = "mcm";
		    core-supply = <&reg_core>;
		    io-supply = <&reg_io>;

			// dsa,mii-bus = <&mdio>;
			dsa,member = <0 0>;
		};

		ephy1: ethernet-phy@1 {
			reg = <1>;
			phy-mode = "rgmii-rxid";
		};

		ephy5: ethernet-phy@7 {
			reg = <7>;
			phy-mode = "rgmii-rxid";

			fixed-link {
				speed = <1000>;
				full-duplex;
			};
		};
	};
};

&mt7530 {

	ports {
		#address-cells = <1>;
		#size-cells = <0>;
		reg = <0>;

		port@0 {
			reg = <0>;
			label = "eth1";
			cpu = <&cpu_port0>;
		};

		port@1 {
			reg = <1>;
			label = "eth2";
			cpu = <&cpu_port0>;
		};

		port@2 {
			reg = <2>;
			label = "eth3";
			cpu = <&cpu_port0>;
		};

		port@3 {
			reg = <3>;
			label = "eth4";
			cpu = <&cpu_port0>;
		};

		port@4 {
			reg = <4>;
			label = "eth5";
			cpu = <&cpu_port0>;
		};

		port@5 {
			reg = <5>;
			label = "eth8";
			cpu = <&cpu_port0>;
			phy-handle = <&ephy5>;

			sfp = <&sfp5>;
       		managed = "in-band-status";

			fixed-link {
				speed = <1000>;
				full-duplex;
			};
		};

		cpu_port0: port@6 {
			reg = <6>;
			label = "cpu";
			ethernet = <&gmac0>;
			phy-mode = "trgmii";

			fixed-link {
				speed = <1000>;
				full-duplex;
			};
		};
	};
};

&pinctrl {
	state_default: pinctrl0 {
		gpio {
			ralink,group = "uart2", "uart3", "i2c", "pcie", "jtag", "rgmii2";
			ralink,function = "gpio";
		};
	};
};
