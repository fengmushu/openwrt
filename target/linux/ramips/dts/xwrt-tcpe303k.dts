/dts-v1/;

#include "mt7628an.dtsi"
  
#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

/ {
	compatible = "mediatek,mt7628an-eval-board", "mediatek,mt7628an-soc";
	model = "Xwrt T-CPE303K";

	chosen {
                bootargs = "console=ttyS0,57600";
        };

	memory@0 {
		device_type = "memory";
		reg = <0x0 0x8000000>;
	};

	gpio-leds {
		compatible = "gpio-leds";

		indicator {
			label = "xwrt:wifi";
			gpios = <&gpio1 12 GPIO_ACTIVE_LOW>;
		};
	}; 

	gpio-i2c {
		compatible = "i2c-gpio";

		gpios = <&gpio0 5 0 /* sda */
			 &gpio0 4 0 /* scl */
			>;

		i2c-gpio,sda-open-drain;
		i2c-gpio,scl-open-drain;
		i2c-gpio,delay-us = <2>;	/* ~100 kHz */

		#address-cells = <1>;
		#size-cells = <0>;
	};
};

&spi0 {
        status = "okay";

        m25p80@0 {
                #address-cells = <1>;
                #size-cells = <1>;
                compatible = "jedec,spi-nor";
                reg = <0>;
                spi-max-frequency = <10000000>;
                m25p,chunked-io = <32>;

                partition@0 {
                        label = "u-boot";
                        reg = <0x0 0x30000>;
                        read-only;
                };

                partition@30000 {
                        label = "u-boot-env";
                        reg = <0x30000 0x10000>;
                        read-only;
                };

                factory: partition@40000 {
                        label = "factory";
                        reg = <0x40000 0x10000>;
                        read-only;
                };

                partition@50000 {
                        label = "firmware";
                        reg = <0x50000 0x1fb0000>;
                };
        };

	spidev@1 {
                #address-cells = <1>;
                #size-cells = <1>;
                compatible = "linux,spidev";
                reg = <1>;
                spi-max-frequency = <10000000>;
        };
};

&uart1 {
        status = "okay";
};

&gpio-i2c {
	status = "okay";

	pca9554: pca9554@3f {
		compatible = "nxp,pca9554";
		reg = <0x74>;
		gpio-controller;
		#gpio-cells = <2>;
	};
};

&ethernet {
        mtd-mac-address = <&factory 0x4>;
        mediatek,portmap = "llllw";
};

&wmac {
        status = "okay";
};

&pinctrl {
	state_default: pinctrl0 {
		gpio {
			ralink,group = "i2c", "i2s", "p0led_an", "p2led_an", "p3led_an", "p4led_an", "wled_an", "gpio", "refclk";
			ralink,function = "gpio";
		};
	};
};
